{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API server for the application. Provides REST API endpoints for authentication, assessments, chat, and user management.",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "npm i && tsx scripts/setup-env.ts && nodemon --exec tsx server.ts",
    "dev:ts": "npm i && tsx scripts/setup-env.ts && nodemon --exec tsx server.ts",
    "dev:e2e": "npm i && tsx scripts/setup-env.ts && cross-env PORT=5005 node server.js --mode e2e",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "db:init": "tsx db/init.ts",
    "db:init:sqlite": "tsx db/init-sqlite.ts",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:migrate": "tsx scripts/run-migrations.ts",
    "db:add-preview": "tsx scripts/add-preview-column.ts",
    "db:check-supabase": "tsx scripts/check-supabase-schema.ts",
    "fix:assessment-schema": "tsx scripts/fix-assessment-schema.ts",
    "fix:schema:simple": "tsx scripts/simple-fix-schema.ts",
    "clear:schema-cache": "tsx scripts/clear-schema-cache.ts",
    "prepare:deploy": "tsx scripts/prepare-deploy.ts",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run -t unit",
    "vercel-build-disabled": "echo 'Custom build disabled for debugging'",
    "vercel-build-simple": "tsx scripts/prepare-deploy.ts",
    "vercel-build-full": "tsx scripts/prepare-deploy.ts && tsx services/vercel-build.ts",
    "___test workflow____": "___test workflow using vitest and test driven development____",
    "___STATUS:PASSED____": "___the following tests have passed____",
    "test:setup:unit": "vitest run routes/setup/__tests__/unit",
    "test:setup:dev": "cross-env TEST_MODE=true vitest run routes/setup/__tests__/e2e/dev",
    "test:setup:prod:real": "cross-env USE_MOCKS=false vitest run routes/setup/__tests__/e2e/prod",
    "test:auth": "vitest run routes/auth/__tests__",
    "test:auth:unit": "vitest run routes/auth/__tests__/unit",
    "test:auth:dev": "cross-env TEST_MODE=true vitest run routes/auth/__tests__/e2e/dev",
    "test:auth:dev:success": "cross-env TEST_MODE=true vitest run routes/auth/__tests__/e2e/dev/success",
    "test:auth:dev:error": "cross-env TEST_MODE=true vitest run routes/auth/__tests__/e2e/dev/error",
    "___STATUS:TODO____": "___the following tests need to be verified____",
    "test:dev": "npx playwright test e2e/dev/master-integration.api.pw.spec.ts -c playwright.dev.config.js",
    "test:dev:verbose": "npx playwright test e2e/dev/master-integration.api.pw.spec.ts -c playwright.dev.config.js --reporter=list --headed",
    "test:dev:with-logs": "tsx scripts/run-dev-tests-with-logs.ts",
    "test:dev:keep": "npx playwright test e2e/dev/master-integration-no-cleanup.api.pw.spec.ts -c playwright.dev.config.js",
    "test:prod": "npx playwright test e2e/dev/master-integration.api.pw.spec.ts -c playwright.prod.config.js",
    "test:prod:verbose": "npx playwright test e2e/dev/master-integration.api.pw.spec.ts -c playwright.prod.config.js --reporter=list --headed",
    "test:prod:keep": "npx playwright test e2e/dev/master-integration-no-cleanup.api.pw.spec.ts -c playwright.prod.config.js",
    "test:prod:deployed": "vercel --prod; npx playwright test e2e/dev/master-integration.api.pw.spec.ts -c playwright.prod.config.js",
    "deploy:vercel": "vercel deploy --prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/vertexai": "^1.10.0",
    "@google/genai": "^0.7.0",
    "@google/generative-ai": "^0.24.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.17.10",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.9.0",
    "cross-env": "^7.0.3",
    "mssql": "^12.0.0",
    "nodemon": "^3.1.9",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.0",
    "tedious": "^18.6.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.1.2"
  }
}
